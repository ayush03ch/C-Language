// to showcase the use of "continue" keyword
//Program to print all numbers in a range, except multiples of a given number
#include<stdio.h>

int main()
{
int n;
printf("Enter the number whose multiple you don't want to print: ");
scanf("%d",&n);
	for(int i=1;i<=100;i++)
	{
		if(i%n==0) 
		continue;
		else printf("%d ",i);
	}
return 0;
}
